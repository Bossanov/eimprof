<!DOCTYPE html>
<html>
  <head>
    <title>Eimprof</title>
    <link rel="stylesheet" href="/assets/camera.css"/>
    <link rel="stylesheet" href="/assets/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="/assets/jquery.js"></script>
    <script src="/assets/jquery-migrate-1.2.1.js"></script>
    <script src="/assets/jquery.easing.1.3.js"></script>

    <script src="/assets/jquery.equalheights.js"></script>
    <script src="/assets/camera.js"></script>
    <script src="/assets/script.js"></script>

    <!--[if (gt IE 9)|!(IE)]><!-->
    <script src="/assets/jquery.mobile.customized.min.js"></script>

    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script>
        $(document).ready(function(){

            jQuery('#camera_wrap').camera({
                loader: false,
                pagination: true,
                minHeight: '',
                thumbnails: false,
                height: '39.375%',
                caption: false,
                navigation: false,
                fx: 'simpleFade',
                playPause: false
            });
        });
    </script>
  </head>

  <body>
    <%= render 'shared/navbar' %>
    <%= yield %>
    <%= render 'shared/footer' %>


  </body>
</html
